generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  code        String
  created_at  DateTime
  email       String
  movies_like Int[]
  name        String
  password    String
}
