generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  code        String
  name        String
  email       String
  password    String
  movies_like Int[]
  created_at  DateTime
}


model movies {
  id                String   @id @default(auto()) @map("_id") @db.ObjectId
  code              Int
  title             String
  original_language String
  original_title    String
  overview          String
  poster_path       String
  like_count        Int
  created_at        DateTime
}
